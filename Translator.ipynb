   "source": [
    "import playsound\n",
    "from deep_translator import GoogleTranslator\n",
    "engine = pyttsx3.init(\"sapi5\")\n",
    "voices = engine.getProperty(\"voices\")\n",
    "engine.setProperty(\"voice\", voices[1].id)\n",
    "\n",
    "def speak(audio):\n",
    "    engine.say(audio)\n",
    "    engine.runAndWait()\n",
    "def wishMe():\n",
    "    hour=int(datetime.datetime.now().hour)\n",
    "    if hour >=0 and hour <12:\n",
    "        speak(\"Good Morning !\")\n",
    "    elif hour>=12 and hour <18:\n",
    "        speak(\"Good Afternoon !\")\n",
    "    else:\n",
    "        speak(\"Good evening !\")\n",
    "    speak(\"I am your Translator, Please tell me How may I help you\")\n",
    "def takecommand():\n",
    "    r=sr.Recognizer()\n",
    "    with sr.Microphone() as source:\n",
    "        print(\"Listening....\")\n",
    "        r.pause_threshold=1\n",
    "        audio=r.listen(source)\n",
    "    try:\n",
    "        print(\"Recognizing...\")\n",
    "        query=r.recognize_google(audio, language=\"en-in\")\n",
    "        print(\"Our Text:\",query)\n",
    "    except Exception as e:\n",
    "        # print(e)\n",
    "        return \"Say it Again\"\n",
    "    return query\n",
    "if __name__==\"__main__\":\n",
    "    wishMe()\n",
    "    while True:\n",
    "        q=takecommand().lower()\n",
    "        if \"exit\" in q:\n",
    "            break\n",
    "        translated_text = GoogleTranslator(source='auto', target='hi').translate(q)\n", # here, hi means hindi language 
    "        print(\"Translated Text:\",translated_text)\n",
    "        tts=gTTS(translated_text)\n",
    "        pygame.mixer.init()\n",
    "        audio_fp = BytesIO()\n",
    "        tts.write_to_fp(audio_fp)\n",
    "        audio_fp.seek(0)\n",
    "        pygame.mixer.music.load(audio_fp)\n",
    "        pygame.mixer.music.play()\n",
    "        while pygame.mixer.music.get_busy():\n",
    "            continue\n",
    "\n",
    "\n"
   ]
